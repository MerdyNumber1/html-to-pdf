[[source]]
url = "https://pypi.org/simple"
verify_ssl = true
name = "pypi"

[scripts]
dev = "adev runserver src --host 0.0.0.0 --port $APP_PORT"
prod = "gunicorn src.asgi:setup_app --bind 0.0.0.0:$APP_PORT --worker-class aiohttp.GunicornWebWorker"

[packages]
aiohttp = "*"
pdfkit-async = "*"
bs4 = "*"
lxml = "*"
loguru = "*"
gunicorn = "*"

[dev-packages]
aiohttp-devtools = "*"

[requires]
